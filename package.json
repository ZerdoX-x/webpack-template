{
  "name": "webpack-template",
  "version": "1.0.0",
  "description": "This is my webpack template I use for each project",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "webpack --config webpack.config.prod.js",
    "dev": "webpack-dev-server --open --progress --color --config webpack.config.dev.js",
    "dev:no-server": "webpack --config webpack.config.dev.js",
    "clean:md": "rimraf src/**/*.md",
    "clean:map": "rimraf dist/**/*.map",
    "clean:dist": "rimraf dist/"
  },
  "keywords": [
    "webpack",
    "boilerplate",
    "starter",
    "template"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ZerdoX-x/webpack-template.git"
  },
  "homepage": "https://github.com/ZerdoX-x/webpack-template#readme",
  "bugs": {
    "url": "https://github.com/ZerdoX-x/webpack-template/issues",
    "email": "zerdox.cool4@gmail.com"
  },
  "author": "zerdox",
  "license": "UNLICENSED",
  "devDependencies": {
    "autoprefixer": "^9.7.0",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.0.4",
    "css-loader": "^3.2.0",
    "css-mqpacker": "^7.0.0",
    "cssnano": "^4.1.10",
    "csv-loader": "^3.0.2",
    "file-loader": "^4.2.0",
    "html-beautify-webpack-plugin": "^1.0.5",
    "html-webpack-plugin": "^3.2.0",
    "image-webpack-loader": "^6.0.0",
    "mini-css-extract-plugin": "^0.8.0",
    "postcss-loader": "^3.0.0",
    "rimraf": "^3.0.0",
    "style-loader": "^1.0.0",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.9",
    "webpack-dev-server": "^3.9.0",
    "webpack-merge": "^4.2.2",
    "xml-loader": "^1.2.1"
  },
  "dependencies": {
    "lodash": "^4.17.15"
  }
}
