{
  "name": "webpack-template",
  "version": "1.0.0",
  "description": "This is my webpack template I use for each project",
  "main": "index.js",
  "sideEffects": false,
  "scripts": {
    "build": "webpack --config webpack.config.prod.js",
    "dev": "webpack-dev-server --open --progress --color --config webpack.config.dev.js",
    "start": "cross-env NODE_ENV=development nodemon --exec \"webpack-dev-server --progress --color --config webpack.config.dev.js\"",
    "*dev": "webpack --config webpack.config.dev.js",
    "clean:dist": "rimraf dist/",
    "clean:info": "rimraf info/",
    "clean:md": "rimraf src/**/*.md",
    "clean:map": "rimraf dist/**/*.map",
    "clean": "npm run clean:dist && npm run clean:info && npm run clean:md && npm run clean:map",
    "info:stats": "make-dir info && webpack-cli --profile --json > info/stats.json || true",
    "info:browsers": "make-dir info && browserslist > info/browserslist.txt",
    "info:versions": "make-dir info && echo 'Node:' > info/versions.txt && node -v >> info/versions.txt && echo 'NPM:' >> info/versions.txt && npm -v >> info/versions.txt && echo 'Webpack:' >> info/versions.txt && webpack -v >> info/versions.txt",
    "info": "npm run info:stats && npm run info:browsers && npm run info:versions",
    "prunecaches": "rimraf ./node_modules/.cache/",
    "postinstall": "npm run prunecaches",
    "postuninstall": "npm run prunecaches"
  },
  "keywords": [
    "webpack",
    "boilerplate",
    "starter",
    "template",
    "front-end"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ZerdoX-x/webpack-template.git"
  },
  "homepage": "https://github.com/ZerdoX-x/webpack-template#readme",
  "bugs": {
    "url": "https://github.com/ZerdoX-x/webpack-template/issues",
    "email": "zerdox.cool4@gmail.com"
  },
  "author": "zerdox",
  "license": "UNLICENSED",
  "devDependencies": {
    "@babel/core": "^7.6.4",
    "@babel/preset-env": "^7.6.3",
    "autoprefixer": "^9.7.0",
    "babel-loader": "^8.0.6",
    "cache-loader": "^4.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.0.4",
    "cross-env": "^6.0.3",
    "css-loader": "^3.2.0",
    "css-mqpacker": "^7.0.0",
    "cssnano": "^4.1.10",
    "csv-loader": "^3.0.2",
    "favicons-webpack-plugin": "^1.0.2",
    "file-loader": "^4.2.0",
    "html-beautify-webpack-plugin": "^1.0.5",
    "html-webpack-plugin": "^3.2.0",
    "image-webpack-loader": "^6.0.0",
    "imports-loader": "^0.8.0",
    "make-dir-cli": "^2.0.0",
    "mini-css-extract-plugin": "^0.8.0",
    "nodemon": "^1.19.4",
    "postcss-loader": "^3.0.0",
    "rimraf": "^3.0.0",
    "style-loader": "^1.0.0",
    "stylelint": "^11.1.1",
    "stylelint-order": "^3.1.1",
    "stylelint-prettier": "^1.1.1",
    "stylelint-webpack-plugin": "^1.0.3",
    "terser-webpack-plugin": "^2.2.1",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.9",
    "webpack-dev-server": "^3.9.0",
    "webpack-merge": "^4.2.2",
    "xml-loader": "^1.2.1"
  },
  "dependencies": {}
}
