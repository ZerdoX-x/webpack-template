    "prod": "rm -rf dist && webpack --mode production --config webpack.config.server.js && cross-env NODE_ENV=production webpack --config webpack.config.prod.js",
    "dev": "webpack --mode development --config webpack.config.server.js",
    "start": "node ./dist/server.js"
  },
